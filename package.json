    "./(src|demo|frame|table)/**/*.js": [
      "eslint --cache --fix",
      "git add"
    ],
    "./(src|demo|frame|table)/(component|css)/**/*.less": [
      "stylelint --cache --fix",
      "git add"
    ]
  },
